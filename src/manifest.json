{
	"manifest_version": 3,
	"name": "Halo Notification Service",
	"host_permissions": ["*://halo.gcu.edu/"],
	"permissions": ["cookies", "identity", "storage"],
	"action": {
		"default_icon": {
			"48": "static/icon.png"
		},
		"default_popup": "popup/index.html"
	},
	"icons": {
		"16": "static/icon.png",
		"32": "static/icon.png",
		"48": "static/icon.png",
		"128": "static/icon.png"
	}
}
